{
  "name": "@beenotung/tslib",
  "version": "10.8.0",
  "description": "utils library in Typescript",
  "keywords": [
    "Typescript",
    "Library",
    "Utility"
  ],
  "author": "Beeno Tung",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/tslib.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/tslib/issues"
  },
  "homepage": "https://github.com/beenotung/tslib#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc -p .",
    "lint": "tslint --project tsconfig.json -c tslint.json",
    "dev": "run-p watch:*",
    "watch:build": "tsc -p . --watch",
    "watch:lint": "chokidar src/**/*.ts -c \"run-s fix lint\" --initial --verbose",
    "fix": "tslint --fix -p .",
    "test": "run-p build lint",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "arraybuffer-to-buffer": "^0.0.4",
    "event-stream": "^3.3.4",
    "isomorphic-fetch": "2.2.1",
    "rxjs": "^5.0.1"
  },
  "devDependencies": {
    "@angular/compiler": "^2.3.1 || >=4.0.0-beta <5.0.0",
    "@angular/core": "^2.3.1 || >=4.0.0-beta <5.0.0",
    "@types/event-stream": "^3.3.33",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/node": "^8.0.53",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "codelyzer": "^3.0.1",
    "npm-run-all": "^4.1.2",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.3.4",
    "zone.js": "^0.8.4"
  },
  "optionalDependencies": {},
  "peerDependencies": {}
}
