{
  "name": "@beenotung/tslib",
  "version": "14.22.0",
  "description": "utils library in Typescript",
  "keywords": [
    "Typescript",
    "Library",
    "Utility"
  ],
  "author": {
    "name": "Beeno Tung",
    "email": "aabbcc1241@yahoo.com.hk",
    "url": "https://beeno-tung.surge.sh"
  },
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/tslib.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/tslib/issues"
  },
  "homepage": "https://github.com/beenotung/tslib#readme",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && gen-index && npm run format",
    "build": "tsc -p .",
    "postbuild": "rm src/index.ts",
    "lint": "tslint --project tsconfig.json -c tslint.json",
    "dev": "run-p watch:*",
    "watch:build": "tsc -p . --watch",
    "watch:lint": "chokidar src/**/*.ts -c \"run-s format lint\" --initial --verbose",
    "format": "prettier --write \"src/**/*.ts\"",
    "postformat": "tslint --fix -p .",
    "test": "run-s build lint",
    "precommit": "run-s format test",
    "commit": "git commit $@",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "arraybuffer-to-buffer": "^0.0.5",
    "axios": "^0.18.0",
    "form-data": "^2.3.3",
    "isomorphic-fetch": "2.2.1",
    "node-localstorage": "^1.3.1",
    "rxjs": "^5.0.1",
    "tape": "^4.10.1"
  },
  "devDependencies": {
    "@beenotung/gen-index": "^1.0.7",
    "@types/cli-progress": "^1.8.1",
    "@types/form-data": "^2.2.1",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/node": "8",
    "@types/tape": "^4.2.33",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "cli-progress": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "tslint": "^5.12.1",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^3.5.1"
  },
  "optionalDependencies": {},
  "peerDependencies": {}
}
