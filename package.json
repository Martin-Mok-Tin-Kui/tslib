{
  "name": "@beenotung/tslib",
  "version": "11.20.2",
  "description": "utils library in Typescript",
  "keywords": [
    "Typescript",
    "Library",
    "Utility"
  ],
  "author": {
    "name": "Beeno Tung",
    "email": "aabbcc1241@yahoo.com.hk",
