  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/tslib.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/tslib/issues"
  },
  "homepage": "https://github.com/beenotung/tslib#readme",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc -p .",
    "lint": "tslint --project tsconfig.json -c tslint.json",
    "dev": "run-p watch:*",
    "watch:build": "tsc -p . --watch",
    "watch:lint": "chokidar src/**/*.ts -c \"run-s format lint\" --initial --verbose",
    "format": "tslint --fix -p .",
    "test": "run-p build lint",
    "precommit": "run-s format test",
    "commit": "git commit $@",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "arraybuffer-to-buffer": "^0.0.4",
    "event-stream": "^4.0.1",
    "isomorphic-fetch": "2.2.1",
    "rxjs": "^5.0.1"
  },
  "devDependencies": {
    "@types/event-stream": "^3.3.33",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/node": "^8.0.53",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "2.3.4",
    "zone.js": "^0.8.4"
  },
  "optionalDependencies": {},
  "peerDependencies": {}
}
