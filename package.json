{
  "name": "@beenotung/tslib",
  "version": "12.5.0",
  "description": "utils library in Typescript",
  "keywords": [
    "Typescript",
    "Library",
    "Utility"
  ],
  "author": {
    "name": "Beeno Tung",
    "email": "aabbcc1241@yahoo.com.hk",
    "url": "https://beeno-tung.surge.sh"
  },
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/tslib.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/tslib/issues"
  },
  "homepage": "https://github.com/beenotung/tslib#readme",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && gen-index",
    "build": "tsc -p .",
    "postbuild": "rm src/index.ts",
    "lint": "tslint --project tsconfig.json -c tslint.json",
    "dev": "run-p watch:*",
    "watch:build": "tsc -p . --watch",
    "watch:lint": "chokidar src/**/*.ts -c \"run-s format lint\" --initial --verbose",
    "format": "prettier --write \"src/**/*.ts\"",
    "postformat": "tslint --fix -p .",
    "test": "run-p build lint",
    "precommit": "run-s format test",
    "commit": "git commit $@",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "arraybuffer-to-buffer": "^0.0.4",
    "isomorphic-fetch": "2.2.1",
    "rxjs": "^5.0.1"
  },
  "devDependencies": {
    "@beenotung/gen-index": "^1.0.4",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/node": "^8.10.38",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.15.3",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^3.2.2",
    "zone.js": "^0.8.4"
  },
  "optionalDependencies": {},
  "peerDependencies": {}
}
